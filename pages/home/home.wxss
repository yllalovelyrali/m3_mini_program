/**index.wxss**/
.banner {
  width: 100%;
  height: 360rpx;
  background-color: #eeeeee;
}
.main_button_panel {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-end;
  background-color: #304444;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
}
.main_button {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 15rpx;
}
.main_scroll_panel {
  width: 100%;
  background-color: #304444;
}
.block_title {
  width: 100%;
  color: #9EC6C6;
  font-size: 28rpx;
}
.bottom_info {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.bottom_text {
  width: 100%;
  color: #9EC6C6;
  font-size: 28rpx;
  text-align: center;
}
.room_btn_block {
  width: 100%;
  background-color: rgba(46, 56, 56, 1.0);
  margin-bottom: 40rpx;
}
.room_block {
  display: flex;
  flex-direction: column;
  background-color: #2E3838;
  align-items: center;
}
.home_main_btn_image {
  width: 120rpx;
  height: 120rpx;
}
.home_btn_image {
  width: 100rpx;
  height: 100rpx;
}
